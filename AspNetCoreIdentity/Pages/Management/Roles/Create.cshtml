@page
@model AspNetCoreIdentity.Pages.Management.Roles.CreateModel
@{
    ViewData["Title"] = "Crear Rol";
}
<form method="post">
    <p></p>
    <h4>Crear Rol</h4>
    <div class="row mb-3">
        <div class="col-md-6">
            <div class="form-floating mb-3 mb-md-0">
                <input type="text" asp-for="Model.RoleName" class="form-control" />
                <label asp-for="Model.RoleName">Nombre</label>
                <span asp-validation-for="Model.RoleName" class="text-danger"></span>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-floating mb-3 mb-md-0">
                <input type="text" asp-for="Model.Description" class="form-control" />
                <label asp-for="Model.Description">Descripcion</label>
                <span asp-validation-for="Model.Description" class="text-danger"></span>
            </div>
        </div>
        <div asp-validation-summary="All" class="text-danger"></div>
    </div>
    <div class="mt-4 mb-0">
        <button type="submit" class="btn btn-primary">Crear Rol</button>
        <a class="btn btn-dark" asp-page="Index">Regresar</a>
    </div>
    <hr />
    <table>
    </table>
</form>
